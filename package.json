    "@rainbow-me/rainbowkit": "^1.1.1",
    "autoprefixer": "10.4.16",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "firebase": "9.18.0",
    "next": "13.5.6",
    "nft.storage": "^7.1.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-image-gallery": "^1.3.0",
    "styled-components": "^6.1.0",
    "tailwindcss": "3.3.3",
    "viem": "~1.12.2",
